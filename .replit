run = "python3 DailyReport/app.py"
modules = ["python-3.11"]

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 8050
externalPort = 3001

[[ports]]
localPort = 8080
externalPort = 8080
exposeLocalhost = true

[[ports]]
localPort = 10000
externalPort = 3000

[nix]
packages = ["freetype", "gitFull", "glibcLocales", "google-cloud-sdk", "jq", "mailutils", "nano", "playwright-driver", "zulu"]

[agent]
expertMode = true

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "VA Bot Server"

[[workflows.workflow]]
name = "VA Bot Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python3 main.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"
